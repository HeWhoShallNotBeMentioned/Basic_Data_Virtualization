<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Detroit Tiger Wins by Year</title>
  </head>
  <body>
    <div id="chart" style="width:800px;height:400px;"></div>
    <script type="text/javascript" src="js/flotr2.js"> </script>
    <script>

    window.onload = function () {
      var wins = [[[0,95], [1,88], [2,74], [3,86], [4,81], [5,95], [6, 88], [7,93], [8,90], [9,74]]];
      var years = [[0,"2006"], [1,"2007"], [2,"2008"], [3,"2009"], [4,"2010"], [5,"2011"], [6,"2012"], [7,"2013"], [8,"2014"], [9,"2015"] ]

      Flotr.draw(
        document.getElementById("chart"), wins, {
          bars: {
            show: true,
            barWidth: 0.5
          },
          yaxis: {
            min: 0,
            tickDecimals: 0
          },
          xaxis: {
            ticks: years
          }
        }
      );
    };
    </script>
  </body>
</html>
