<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Can you see me?</title>
  </head>
  <body>
    <div id="chart" style="width:800px;height:400px;"></div>
    <script src='js/flotr2.min.js'> </script>
    <script>

    window.onload = function () {
      var wins = [[[2006,13], [2007,11], [2008,15], [2009,15], [2010,18], [2011,21], [2012, 28], [2013,23], [2014,27], [2015,24]]];

      Flotr.draw(
        document.getElementById("chart"),
        wins,
        {
          bars: {
            show: true
          }
        }
      );
    };
    </script>
  </body>
</html>
